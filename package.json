    "start": "node server.js",
    "heroku-postbuild": "cd client && yarn install && yarn install --only=dev --no-shrinkwrap && yarn build",
    "logger": "scripting/commands/logger.js"
  },
  "dependencies": {
    "address": "^1.0.3",
    "axios": "^0.18.0",
