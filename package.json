  "devDependencies": {
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "commander": "^2.18.0",
    "concurrently": "^3.6.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "puppeteer": "^1.8.0"
	},
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/wayn/drop-it-like-its-hot.git"
